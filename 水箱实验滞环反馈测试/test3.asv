C1 = pid(0.033, 0.046 / 98.4, 0);
C2 = pid(0.0375, 0.1 / 68.2, 0);
G = tf(54.3, [123, 1]);
T1 = G * C1 / (1 + G * C1);
T2 = G * C2 / (1 + G * C2);
splInvl = 1.1748;
time = (0 : 323)' * splInvl;
y1 = step(T1, time) + rand(324, 1) * 0.3;
plot(time, y1);
y1 = step(T2, time) + rand(324, 1) * 0.3;
plot(time, y1);